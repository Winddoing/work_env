CompileFlags:
  Remove: 
    - '-mabi=lp64'
    - '-fno-allow-store-data-races'
    - '-fmin-function-alignment=*'     # 使用通配符移除所有变体
    - '-fconserve-stack'
    - '-femit-struct-debug-*'          # 通配符匹配所有debug选项
    - '-fno-delete-null-pointer-checks'
    - '-fno-strict-overflow'
    - '-fno-stack-clash-protection'
    - '-fmacro-prefix-map=*'           # 移除路径映射选项
  Add:
    - '-Wno-unknown-warning-option'    # 忽略未知警告
    - '-D__KERNEL__'
    - '-D__clang__'                    # 模拟Clang环境
